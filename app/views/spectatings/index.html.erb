<p id="notice"><%= notice %></p>

<h1>Listing Spectatings</h1>

<table>
  <thead>
    <tr>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Address</th>
      <th>Title</th>
      <th>Description</th>
      <th>Content</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @spectatings.each do |spectating| %>
      <tr>
        <td><%= spectating.latitude %></td>
        <td><%= spectating.longitude %></td>
        <td><%= spectating.address %></td>
        <td><%= spectating.title %></td>
        <td><%= spectating.description %></td>
        <td><%= spectating.content %></td>
        <td><%= spectating.date %></td>
        <td><%= link_to 'Show', spectating %></td>
        <td><%= link_to 'Edit', edit_spectating_path(spectating) %></td>
        <td><%= link_to 'Destroy', spectating, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Spectating', new_spectating_path %>

<div style='width: 100%;'>
  <div id="map" style='width: 100%; height: 400px;'></div>
</div>



<script src="//maps.google.com/maps/api/js?key=AIzaSyAdyx9mz5wkrBVL3_SU58Freie2JRyABQs "></script>
<script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>
<script src='//cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js' type='text/javascript'></script> <!-- only if you need custom infoboxes -->


<script>
handler = Gmaps.build('Google');
handler.buildMap({ provider: {}, internal: {id: 'map'}}, function(){
   markers = handler.addMarkers(<%=raw @hash.to_json %>);
  handler.bounds.extendWith(markers);
  handler.fitMapToBounds();
});


</script>